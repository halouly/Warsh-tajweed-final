{
  "id": 111,
  "name_ar": "المَسَد",
  "name_en": "Al-Masad",
  "verses": [
    "تَبَّتْ يَدَآ أَبِے لَهَبٖ وَتَبَّۖ",
    "مَآ أَغْن۪ىٰ عَنْهُ مَالُهُۥ وَمَا كَسَبَۖ",
    "سَيَصْلَىٰ نَاراٗ ذَاتَ لَهَبٖ",
    "وَامْرَأَتُهُۥۖ حَمَّالَةُ اُ۬لْحَطَبِ",
    "فِے جِيدِهَا حَبْلٞ مِّن مَّسَدٖۖ"
  ]
}